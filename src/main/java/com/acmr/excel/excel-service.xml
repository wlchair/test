<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:jdbc="http://www.springframework.org/schema/jdbc" xmlns:jee="http://www.springframework.org/schema/jee"
	xmlns:tx="http://www.springframework.org/schema/tx" xmlns:jpa="http://www.springframework.org/schema/data/jpa"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd
	http://www.springframework.org/schema/jdbc http://www.springframework.org/schema/jdbc/spring-jdbc-4.0.xsd
	http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-4.0.xsd
	http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-4.0.xsd
	http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc.xsd
	http://www.springframework.org/schema/data/jpa http://www.springframework.org/schema/data/jpa/spring-jpa.xsd"
	default-lazy-init="false">
	<bean id="excelDao" class="com.acmr.excel.dao.ExcelDaoImpl">
		 <property name="dataSource" ref="dataSource"/>
	</bean>
	<bean id="handleExcelService" class="com.acmr.excel.service.HandleExcelService"/>
	<bean id="downLoadExcelService" class="com.acmr.excel.service.DownLoadExcelService"/>
	<bean id="cellService" class="com.acmr.excel.service.CellService"/>
	<bean id="sheetService" class="com.acmr.excel.service.SheetService"/>
	<bean id="pasteService" class="com.acmr.excel.service.PasteService"/>
	<bean id="excelService" class="com.acmr.excel.service.ExcelService">
		<property name="excelDao" ref="excelDao"/>
	</bean>
	
	
</beans>